<?xml version="1.0"?>
<launch>

		<!-- reads serial port of MCU -->
<node pkg="gpar_mcuserial" type="mcuserial_node" name="mcuserial_node" >
   <param name="serial_port" type="string" value="/dev/ttyACM1"/>
   </node>

   <!-- Frame = k64f_imu (name has to be k64f_imu for kalman node .. TODO parametrize -->
  <node pkg="gpar_k64f" type="k64f_imu" name="k64f_imu" output="screen" />


  <!-- Applies kalman filter on IMU data from k64f_imu node -->
  <node pkg="gpar_kalman" type="kalman_k64f" name="kalman_k64f" output="screen" />

		  <!--  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find gpar_kalman)/rviz/tf.rviz" /> -->

</launch>
